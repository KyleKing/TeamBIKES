{
    "blocks": false,
    "brackets": "never",
    "colons": "never",
    "colors": "always",
    "commaSpace": "always",
    "commentSpace": "always",
    "cssLiteral": "never",
    "depthLimit": 6,
    "duplicates": true,
    "efficient": "always",
    "extendPref": "@extends",
    "globalDupe": true,
    "indentPref": false,
    "leadingZero": "always",
    "maxErrors": false,
    "maxWarnings": false,
    "mixed": false,
    "namingConvention": "lowercase-dash",
    "namingConventionStrict": true,
    "none": "never",
    "noImportant": true,
    "parenSpace": "always",
    "placeholders": "always",
    "prefixVarsWithDollar": "always",
    "quotePref": "single",
    "semicolons": "never",
    "sortOrder": "alphabetical",
    "stackedProperties": "never",
    "trailingWhitespace": "never",
    "universal": false,
    "valid": true,
    "zeroUnits": "never",
    "zIndexNormalize": 1
}