.mapContainer
  position fixed
  // space for bottom bar
  bottom 40px
  // space for nav bar
  top MobileNavHeight
  +above(750px)
    top: NavHeight
  left 0
  right 0
  height auto

.mapContainerFull
  position fixed
  bottom 0
  left 0
  right 0
  // space for nav bar
  top MobileNavHeight
  +above(750px)
    top: NavHeight
  height auto

.full-screen-map
  height 100%
  width 100%

.float-map
  position float
  height 2*300px

#reserve
	position fixed
	left 0
	right 0
	height 40px
	bottom 0px

