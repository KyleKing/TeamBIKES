template(name='admin2layoutBikeData')
  .template-home
    .page-header
      h1 Admin 2 - Bike Data
  //- a(href="{{pathFor 'Test'}}")
    button.seeLess See Less

  #with
    table.table.table-hover.table-condensed
      thead
        //- Per user history
        tr
          th bike
          th timestamp
          th User
          th Lat
          th Lng
      tbody
        each this.positions
          tr
            td #{bike}
            td {{formatDate timestamp "short"}}
            //- td #{timestamp}
            td #{user}
            td #{lat}
            td #{lng}