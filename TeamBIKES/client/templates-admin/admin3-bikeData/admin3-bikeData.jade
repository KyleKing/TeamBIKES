template(name='admin3layoutBikeData')
  .template-home
    .page-header
      h1 Admin 3 - Bike Data
    a(href="{{pathFor 'admin3layoutAllData'}}")
      button.seeLess Back

  #with
    table.table.table-hover.table-condensed
      thead
        //- Per user history
        tr
          th Timestamp
          th User
          th Lat.
          th Lng.
      tbody
        each this.positions
          tr
            td {{formatDate timestamp "short"}}
            //- td #{timestamp}
            td #{user}
            td #{lat}
            td #{lng}