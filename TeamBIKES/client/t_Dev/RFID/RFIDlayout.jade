template(name='RFIDlayout')
  .template-home
    .page-header
      h1 RFID Demo
  table.table.table-hover.table-condensed
    thead
      //- Labels Row
      tr
        th USER_ID
        th LATITUDE
        th LONGITUDE
        th LOCKSTATEE
        th TIMESTAMP
    tbody
      each RFIDlayout
        tr
          td #{USER_ID}
          td #{LATITUDE}
          td #{LONGITUDE}
          td #{LOCKSTATEE}
          td #{TIMESTAMP}