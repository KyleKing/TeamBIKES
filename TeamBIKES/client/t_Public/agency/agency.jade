template(name='agency')
  .jumbotron
    .container
      h1 RedBars is the smartest, fastest, and most convenient way to get around campus.
      button(type="button" href="{{ pathFor 'atSignUp' }}").btn.btn-primary.btn-lg
        span Sign Up for Free

  //- header.agency
